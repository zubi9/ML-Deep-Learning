function determineAction(user_type, action_url) {
  var user_action_text = "";
  for (var i = 0; i < fmi4_sublist[user_type].length; i++) {
    if (fmi4_sublist[user_type][i].url === action_url) {
      user_action_text = fmi4_sublist[user_type][i].text;
      break;
    }
  }
  return user_action_text;
}

/*
	Populates useraction form based on usertype
*/
function fmi4_update_useraction(form) {
  var user_type = form.value;
  var user_action_dropdown = document.getElementById("fmi4_useraction");

  while (user_action_dropdown.getElementsByTagName("option").length > 0) {
    user_action_dropdown.removeChild(
      user_action_dropdown.getElementsByTagName("option")[0]
    );
  }

  var option = document.createElement("option");
  option.value = "";
  option.innerHTML = "I want to...";
  user_action_dropdown.appendChild(option);

  for (var i = 0; i < fmi4_sublist[user_type].length; i++) {
    option = document.createElement("option");
    option.value = fmi4_sublist[user_type][i].url;
    option.innerHTML = fmi4_sublist[user_type][i].text;
    user_action_dropdown.appendChild(option);
  }

  document.getElementById("fmi4_useraction").selectedIndex = 0;
}

function fmi4_useraction_chosen(form) {
  var user_action = form.options[form.selectedIndex].text;
  if (user_action.length > 27) {
    fmi4_useraction_fulltext = user_action;
    form.options[form.selectedIndex].text = user_action.substr(0, 24) + "...";
  }
  //form.blur();
}

function fmi4_useraction_open(i) {
  if (fmi4_useraction_fulltext !== "") {
    var user_action = document.getElementById("fmi4_useraction");
    user_action.options[
      user_action.selectedIndex
    ].text = fmi4_useraction_fulltext;
    fmi4_useraction_fulltext = "";
  }
}

if (document.getElementById("fmi4_usertype").value !== "") {
  fmi4_update_sublist();
}

function fmi4_click() {
  var user = document.getElementById("fmi4_usertype").value;
  var user_action_url = document.getElementById("fmi4_useraction").value;
  var user_action = determineAction(user, user_action_url);
  var action_string =
    location.href + "- how can we help you [" + user + ", " + user_action + "]";
  if (user !== "") {
    s.tl(true, "o", action_string, null);
    location.href = user_action_url;
  }
}
